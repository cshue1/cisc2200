#ifndef LIST_H
#define LIST_H

#define MAX_NAMES 9

class list
{
public:
    list();
    bool isEmpty();
    int getLength();
    bool insert(int, string);
    bool remove (int);
    bool clear();
    string getEntry(int);
    bool replace(int, string);
    void display();
private:
    int size;
    string names[MAX_NAMES];
};

#endif
